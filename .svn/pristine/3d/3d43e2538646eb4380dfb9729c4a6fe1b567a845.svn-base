/*css 初始化 */

html,
body,
ul,
li,
ol,
dl,
dd,
dt,
p,
h1,
h2,
h3,
h4,
h5,
h6,
form,
fieldset,
legend,
img,
input {
    margin: 0;
    padding: 0;
}

fieldset,
img,
input,
button {
    border: none;
    padding: 0;
    margin: 0;
    outline-style: none;
    /*外面环绕线*/
}

ul,
ol,li {
    list-style: none;
}

img {
    vertical-align: middle;
    border: 0;
}

table {
    border-collapse: collapse;
    /*合并外连线*/
}

body {
    font: 12px/150% "microsoft yahei";
    color: #666;
    /* background: #F6F6F6; */
}


/* 清除浮动的影响 */

.clearfix:after {
    content: "";
    display: block;
    clear: both;
    line-height: 0;
    visibility: hidden;
    height: 0;
}

a {
    color: #333;
    text-decoration: none;
}

a:hover {
    text-decoration: none;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-weight: normal;
}

s,
i,
em {
    font-style: normal;
    text-decoration: none;
}

