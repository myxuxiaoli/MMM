    <header id="header" class="clearfix">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xs-6 logo">
                    <a href="#">
                        <img src="../images/header_logo.png" alt="" class="">
                    </a>
                </div>
                <div class="col-xs-6 appdown">
                    <a href="#">
                        <img src="../images/header_app.png" alt="">
                    </a>
                </div>
            </div>
        </div>
    </header>
    <!-- 搜索框 -->
    <div class="search">
