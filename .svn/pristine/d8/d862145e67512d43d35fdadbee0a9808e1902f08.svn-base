            </div>
            <div class="searchbtn">
                <input type="submit" value="搜索">
            </div>
        </div>
    </div>

    <!-- 主体内容 -->
    <form action="" id="aspnetForm" ng-view>

    </form>
    <!-- 路径导航栏 -->
    <script src="./assets/lib/angular/angular-route/angular-route.min.js"></script>
    <script src="./app.js"></script>
    <script src="./home/home.js"></script>
    <script src="./getbrand/getbrand.js"></script>
    <script src="../../node_modules/angular-sanitize/angular-sanitize.min.js"></script>
